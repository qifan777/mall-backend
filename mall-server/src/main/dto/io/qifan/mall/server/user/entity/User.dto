
input UserInput {
    #allScalars(User)
    id?
    roleIds: Array<String>
}
input UserRegisterInput{
    phone
    password
    code: String
}
specification UserSpec {
    #allScalars
    like/i(id)
    like/i(phone)
//    like/i(gender)
    like/i(nickname)
    like/i(password)
    ge(createdTime)
    le(createdTime)
    ge(editedTime)
    le(editedTime)
}